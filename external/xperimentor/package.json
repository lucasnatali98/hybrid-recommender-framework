{
  "name": "xperimentor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "DEBUG=xperimentor:* node ./bin/www",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha -r ts-node/register test/**/*.ts",
    "coverage": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' nyc -r lcov -e .ts -x \"*.ts\" mocha -r ts-node/register test/**/*.ts && nyc report"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "ace-builds": "^1.4.5",
    "bootstrap": "^4.3.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.4.1",
    "js-yaml": "^3.13.1",
    "morgan": "~1.9.1",
    "pug": "^2.0.4",
    "requirejs": "^2.3.6",
    "serialazy": "^2.0.1",
    "toastr": "^2.1.4",
    "visjs-network": "^4.24.10"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.10.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.1.32",
    "@types/node": "^12.6.9",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.1",
    "gulp-watch": "^5.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
